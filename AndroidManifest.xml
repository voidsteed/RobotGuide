<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.robotguide"
    android:versionCode="1"
    android:versionName="1.0" >
<uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
<uses-permission android:name="android.permission.INTERNET" />
<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo" >
        <activity
            android:name="com.example.robotguide.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.robotguide.DisplayButtonMapActivityActivity"
            android:label="@string/title_activity_display_button_map_activity" >
        </activity>
        <activity
            android:name="com.example.robotguide.DisplayButtonMapActivity"
            android:label="@string/title_activity_display_button_map" >
        </activity>
        <activity
            android:name="com.example.robotguide.DisplayActivityMainActivity"
            android:label="@string/title_activity_display_activity_main" >
        </activity>
        <activity
            android:name="com.example.robotguide.NameListActivity"
            android:label="@string/title_activity_name_list" >
        </activity>
        <activity
            android:name="com.example.robotguide.NameFilterList"
            android:label="@string/title_activity_name_filter_list" >
        </activity>
        <activity
            android:name="com.example.robotguide.MapOverlay"
            android:label="@string/title_activity_map_overlay" >
        </activity>
    </application>
</manifest>
